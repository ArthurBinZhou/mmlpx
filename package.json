{
  "name": "mmlpx",
  "version": "0.0.1",
  "description": "mobx model layer paradigm",
  "main": "./index.js",
  "private": true,
  "scripts": {
    "build:develop": "webpack --config build/webpack-dev.config.js --progress --env develop",
    "build:qa": "webpack --config webpack-build.config.js --progress --env qa",
    "build:stage": "webpack --config webpack-build.config.js --progress --env stage",
    "build:production": "webpack --config webpack-build.config.js --progress --env production",
    "lint": "eslint src",
    "start": "node server.js",
    "test": "npm run lint",
    "prepush": "npm run lint"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "lru-cache": "^4.1.1",
    "mobx": "^3.2.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-mobx-deep-action": "^1.5.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.1.1",
    "eslint-config-shuyun-angular": "^3.0.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-babel": "^4.1.1",
    "express": "^4.15.3",
    "husky": "^0.14.1",
    "json-mock-kuitos": "^1.2.0"
  },
  "eslintConfig": {
    "extends": "shuyun-angular",
    "plugins": [
      "babel"
    ],
    "rules": {
      "new-cap": "off",
      "no-process-env": "off",
      "babel/no-invalid-this": 1,
      "babel/semi": 1
    }
  },
  "eslintIgnore": [
    "build/*"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
