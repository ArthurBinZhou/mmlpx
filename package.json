{
  "name": "mmlpx",
  "version": "2.0.2",
  "description": "mobx model layer paradigm",
  "homepage": "https://github.com/mmlpxjs/mmlpx",
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "scripts": {
    "build": "npm run tsc && cp {package.json,README.md,.npmignore} dist",
    "tsc": "npm run tsc:cjs & npm run tsc:mjs",
    "tsc:cjs": "rm -fr dist/lib && tsc",
    "tsc:mjs": "rm -fr dist/esm && tsc -p tsconfig.mjs.json",
    "start": "tsc -w && ava -w",
    "pub": "npm run lint && npm run build && npm publish dist",
    "prepush": "npm run lint",
    "test": "tsc && nyc ava -v",
    "test:pure": "tsc && ava",
    "report": "nyc report --reporter=html",
    "codecov": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "coverageCheck": "nyc --check-coverage --lines 80 --functions 80 --branches 80 npm run test:pure",
    "lint": "tslint ./src & npm test"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "lru-cache": "^4.1.1",
    "mobx": "^3.4.1",
    "tslib": "^1.8.1",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.91",
    "@types/lru-cache": "^4.1.0",
    "@types/sinon": "^4.1.2",
    "@types/uuid": "^3.4.3",
    "ava": "^0.24.0",
    "codecov": "^3.0.0",
    "husky": "^0.14.1",
    "nyc": "^11.4.1",
    "sinon": "^4.1.3",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.6.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "ava": {
    "files": "dist/lib/**/__tests__/**/*.js"
  }
}
