{
  "name": "mmlpx",
  "version": "0.1.0",
  "description": "mobx model layer paradigm",
  "homepage": "https://github.com/mmlpx/mmlpx",
  "main": "./index.js",
  "scripts": {
    "build": "rm -fr lib && NODE_ENV=production babel ./ --out-dir lib --ignore **/__tests__,node_modules && cp package.json lib/package.json",
    "lint": "eslint ./",
    "start": "node server.js",
    "test": "npm run lint",
    "pub": "npm run lint && npm run build && npm publish lib",
    "prepush": "npm run lint"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "lru-cache": "^4.1.1",
    "mobx": "^3.2.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-mobx-deep-action": "^1.5.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.1.1",
    "eslint-config-shuyun": "^3.0.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-babel": "^4.1.1",
    "express": "^4.15.3",
    "husky": "^0.14.1"
  },
  "eslintConfig": {
    "extends": "shuyun"
  },
  "eslintIgnore": [
    "lib/*"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
