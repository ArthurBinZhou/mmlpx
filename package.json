{
  "name": "mmlpx",
  "version": "1.0.2",
  "description": "mobx model layer paradigm",
  "homepage": "https://github.com/mmlpxjs/mmlpx",
  "main": "./index.js",
  "scripts": {
    "build": "rm -fr lib && NODE_ENV=production babel ./src --out-dir lib --ignore node_modules && cp package.json lib/package.json && cp README.md lib/README.md",
    "start": "NODE_ENV=test ava -w",
    "pub": "npm run lint && npm run build && npm publish lib",
    "prepush": "npm run lint",
    "test": "NODE_ENV=test nyc ava -v",
    "report": "nyc report --reporter=html",
    "codecov": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "coverageCheck": "nyc --check-coverage --lines 80 --functions 80 --branches 80 npm test",
    "lint": "eslint ./src & npm test"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "lru-cache": "^4.1.1",
    "mobx": "^3.2.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-mobx-deep-action": "^1.5.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-stage-0": "^6.24.1",
    "codecov": "^2.3.0",
    "eslint": "^4.1.1",
    "eslint-config-shuyun": "^3.0.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-babel": "^4.1.1",
    "express": "^4.15.3",
    "husky": "^0.14.1",
    "nyc": "^11.1.0",
    "sinon": "^3.2.1"
  },
  "eslintConfig": {
    "extends": "shuyun"
  },
  "eslintIgnore": [
    "lib/*"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "ava": {
    "require": [
      "babel-register"
    ],
    "files": [
      "src/**/__tests__/**/*.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "babel": {
      "babelrc": true,
      "env": {
        "test": {
          "sourceMaps": "inline"
        }
      }
    }
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/node_modules/**",
      "examples/**",
      "**/__tests__/**"
    ],
    "all": true,
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
